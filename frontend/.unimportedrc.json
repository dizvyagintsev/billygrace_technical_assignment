{
  "rootDir": ".",
  "aliases": {
    "src/*": ["./src", "./src/*"]
  },
  "entryFiles": [
    {
      "file": "src/index.tsx",
      "aliases": {
        "src/*": ["./src", "./src/*"]
      },
      "extensions": [".js", ".jsx", ".ts", ".tsx"]
    }
  ],
  "moduleDirectory": ["node_modules"],
  "ignorePatterns": ["**/node_modules/**", "**/*.d.ts"]
}